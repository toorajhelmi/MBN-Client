        <StackLayout Spacing="20" Padding="15" VerticalOptions="Center">
            <Entry Placeholder="Account #" x:Name="Account" />
            <Button Text="Login" Clicked="Handle_Clicked" />
        </StackLayout>
    </ContentView.Content>
</ContentPage>